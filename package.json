{
  "name": "s3-tooling",
  "version": "0.1.0",
  "main": "./dist/src/main.js",
  "scripts": {
    "dev": "concurrently \"vite build --watch\" \"vite build -c ./vite.web.config.ts --watch\" \"tsc -p ./src/main/tsconfig.json --watch\" \"tsc -p ./src/renderer/tsconfig.json --watch\" \"dotenvx run -f .dev.env -- electron .\"",
    "build": "tsc -p ./src/main/tsconfig.json && tsc -p ./src/renderer/tsconfig.json && vite build && vite build -c ./vite.web.config.ts",
    "test": "vitest",
    "lint": "eslint .",
    "start": "dotenvx run -f .dev.env -- electron .",
    "start:web": "dotenvx run -f .dev.env -- node ./dist/src/main.js webonly",
    "screenshots": "dotenvx run -f .dev.env -- electron ./dist/src/main-screenshots.js",
    "screenshots2": "dotenvx run -f .dev.env -- node ./dist/src/main-screenshots2.js",
    "screenshots3": "dotenvx run -f .dev.env -- node ./dist/src/main-screenshots3.js"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@types/express": "^5.0.3",
    "@types/node": "^22.13.17",
    "@types/passport": "^1.0.17",
    "@types/passport-github2": "^1.2.9",
    "concurrently": "^9.1.2",
    "electron": "^35.1.5",
    "eslint": "^9.35.0",
    "jsdom": "^26.1.0",
    "typescript": "^5.2.2",
    "typescript-eslint": "^8.43.0",
    "vite": "^6.2.6",
    "vitest": "^3.1.1"
  },
  "dependencies": {
    "@dotenvx/dotenvx": "^1.39.1",
    "@lit/context": "^1.1.5",
    "@octokit/rest": "^21.1.1",
    "@vaadin/router": "^2.0.0",
    "chart.js": "^4.4.9",
    "connect-sqlite3": "^0.9.16",
    "express": "^5.1.0",
    "express-session": "^1.18.2",
    "lit": "^3.1.4",
    "passport": "^0.7.0",
    "passport-github2": "^0.1.12",
    "sqlite3": "^5.1.7"
  }
}
